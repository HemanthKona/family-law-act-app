                            "isRequired": true
                        },
                        {
                            "type": "yesno",
                            "name": "inCourtForPO",
                            "title": "Were you in court when the Protection Order was made?",
                            "isRequired": true
                        },
                        {
                            "type": "comment",
                            "name": "whyNotInCourt",
                            "isRequired": true,
                            "visibleIf": "{inCourtForPO} = \"n\"",
                            "title": "Please explain why you were not in Court"
